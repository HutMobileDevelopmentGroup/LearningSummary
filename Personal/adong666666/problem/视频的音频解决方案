      
      
      import AVFoundation
      
      
      
      do {
            _ = try AVAudioSession.sharedInstance().setCategory(AVAudioSessionCategoryPlayback, with: AVAudioSessionCategoryOptions.mixWithOthers)
        } catch {
            print("音视频设置捕获异常！！！！！！！")
        }
        
        
        
