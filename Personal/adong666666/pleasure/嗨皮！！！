        Alamofire.request(url).responseJSON
            { response in
                debugPrint(response)
                
                if let json2 = response.result.value
                {
                    print("JSON: \(json2)")
                    let json = JSON(json2)
                    //let price = json2["show_list_after"][0]["room_list"][0]["price"]
                    let aaa = json["show_list_after"][0]["big_title"]
